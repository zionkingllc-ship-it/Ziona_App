{
  "expo": {
    "scheme": "ziona",
    "name": "Ziona",
    "slug": "ziona",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "ios": {
      "bundleIdentifier": "com.zionking.ziona",
      "associatedDomains": [
        "applinks:ziona.app"
      ]
    },
    "android": {
      "package": "com.zionking.ziona",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "dev.ziona.app",
              "pathPrefix": "/post"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-video",
      "expo-audio",
      "react-native-video"
    ],
    "extra": {
      "router": {},
      "eas": {
        
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    }
  }
}
